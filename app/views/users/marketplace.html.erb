<div class="off-canvas-wrap ">
  <div class="inner-wrap">
    <%= render partial: "topbar" %>
    <aside class="left-off-canvas-menu">
      <ul class="off-canvas-list">
        <%= render partial: "topbar_links" %>
        <%= render partial: "marketplace_nav" %>
      </ul>
    </aside>
    <section class="main-section">
      <%= render partial: "hero" %>
      <%= render partial: "profile_banner" %>
      <div id="marketplace" class="row">
        <!-- Side Bar -->
        <div id="marketplace-nav" class="medium-4 large-3 columns hide-for-small">
          <div class="panel">
            <%= render partial: "marketplace_nav" %>
          </div>
        </div>
        <!-- End Side Bar -->
        <div class="small-12 medium-8 large-9 columns">
          <%= render partial: "marketplace_search_filter" %> <!-- TODO Need to change distance to user's distance -->
          <%= render partial: "marketplace_items" %>
        </div>
      </div>
      <%= render partial: "footer" %>
    </section>
    <a class="exit-off-canvas"></a>
  </div>
</div>